#include <stdio.h>

int main() {
    // Write C code here
    int a;
    printf("enter a number=");
    scanf("%d", &a);
    if(a%2==0)
        {
            printf("your number is even\n");
    
        }
    else
        {
            printf(" your number is odd");    
        }

    return 0;
}
