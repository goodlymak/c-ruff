#include <stdio.h>

int main() {
    // Write C code here
    float b;
    float c;
    float a;
    float p;
    printf("enter lenght of the rectangl=");
    scanf("%f",&b);
    printf("enter breadth of rectangle");
    scanf("%f",&c);  
     a=b*c;
     p=2*(b+c);
